{
  "name": "react-with-pika",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "serve ./dest/",
    "prepare": "mkdirp dest && pika install --dest dest/web_modules/ && npm run build-view && npm run cp-files",
    "cp-files": "cpx src/index.html dest/",
    "build-view": "babel ./src/ --out-dir ./dest/ --ignore ./src/index.html"
  },
  "dependencies": {
    "react": "npm:@reactesm/react@^0.1.0",
    "react-dom": "npm:@reactesm/react-dom@^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@pika/web": "^0.6.1",
    "cpx": "^1.5.0",
    "mkdirp": "^0.5.1",
    "serve": "^11.2.0"
  }
}
